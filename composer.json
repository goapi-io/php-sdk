{
    "name": "goapi-io/php-sdk",
    "description": "GOAPI.IO PHP SDK",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GOAPI\\IO\\": "src/"
        }
    },
    "authors": [
        {
            "name": "daniwebdev",
            "email": "dani.webdev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "pest": "./vendor/bin/pest",
        "test-region": "./vendor/bin/pest tests/IndonesianRegionTest.php",
        "test-idx": "./vendor/bin/pest tests/StockIDXTest.php",
        "test-places": "./vendor/bin/pest tests/PlacesTest.php"
    }
}
